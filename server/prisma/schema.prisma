generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Tempo {
  FAST
  MEDIUM
  SLOW
}

model Song {
  id               String  @id @default(uuid())
  title            String
  artist           String?
  tempo            Tempo?
  range            Int?
  alternativeRange Int?
  key              String?
}
